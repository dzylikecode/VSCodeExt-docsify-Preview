{
  "name": "docsify-preview",
  "displayName": "docsify-Preview",
  "description": "write docs easily with docsify",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docsify-preview.sidePreview",
        "title": "Show side preview",
        "category": "menubar",
        "icon": {
          "light": "./assets/Preview.svg",
          "dark": "./assets/Preview_inverse.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "docsify-preview.sidePreview",
          "when": "editorLangId == markdown",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.3"
  }
}
